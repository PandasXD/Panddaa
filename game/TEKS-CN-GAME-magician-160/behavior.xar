                                                                                                            <script language="4">
                                                                                                                <content>
                                                                                                                    <![CDATA[import time

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.audioplayer = ALProxy("ALAudioPlayer")
        self.memory = ALProxy("ALMemory")

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        try:
            self.memory.removeData("Sound_id")
        except:
            #self.logger.warning("Sound_taskId didn't remove.")
            pass
        pass

    def onInput_onFadeout(self):

        try:
            # get PlaySound's ID
            self.Sound_taskId = self.memory.getData("Sound_id")
            #self.logger.info("Sinit:Sound_taskId = " + str(self.Sound_taskId))

             # get Now PlaySound's Volume
            self.SoundVol_b = self.audioplayer.getVolume(self.Sound_taskId)
            #self.logger.info("Sinit:SoundVol_b = " + str(self.SoundVol_b))

            self.wk_time = self.getParameter("Time")
            #self.logger.info("self.wk_time = " + str(self.wk_time))

            self.startTime = time.time()
            #self.logger.info("startTime = " + str(self.startTime))

            nowTime = self.startTime
            now_vol = round(self.SoundVol_b, 2)

            while ((nowTime - self.startTime) <= self.wk_time):

                #self.logger.info("nowTime = " + str(nowTime))

                nowVolPer = (nowTime - self.startTime)/ self.wk_time
                #self.logger.info("nowVolPer = " + str(nowVolPer))


                #self.logger.info("onSet:get_vol = " + str(now_vol))
                if now_vol < 0.001:
                    now_vol = 0.000
                else:
                    now_vol = self.SoundVol_b * (1-nowVolPer)

                self.audioplayer.setVolume(self.Sound_taskId, now_vol)

                if now_vol == 0.000:
                    break # activate output of the box
                else:
                    nowTime = time.time()
                    #self.logger.info("now_vol = " + str(now_vol))
                    time.sleep(0.05)

            try:
                self.memory.removeData("Sound_id")
            except:
                self.logger.warning("Sound_taskId didn't remove.")
            finally:
                self.onEnd()

        except:
            self.logger.warning("onInput_onFadeout Error.")
            self.onEnd()
        pass]]>
</content>
