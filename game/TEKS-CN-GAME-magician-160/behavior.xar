                                                <bitmap>media/images/box/box-script.png</bitmap>
                                                <script language="4">
                                                    <content>
                                                        <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.memory = None
        self.isError = False

    def onLoad(self):
        try:
            self.memory = ALProxy("ALMemory")
        except:
            self.isError = True

    def onUnload(self):
        self.sweep()

    def onInput_onStart(self):
        pass

    def sweep(self):
        if self.isError:
            self.logger.error("Cannot be initialized.")
            self.onError()
            return

        try:
            for key in self.memory.getDataList(self.getParameter("Key")):
                self.logger.info("Remove ALMemory Data. key: " + str(key))
                self.memory.removeData(key)

            self.onSuccess()
        except:
            self.logger.error("Cannot remove data.")
            self.onError()]]>
</content>
